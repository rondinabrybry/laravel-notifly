{
    "name": "brybry/laravel-notify",
    "description": "A Laravel package for real-time notifications, events, and chat functionality using self-hosted WebSocket server",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "websocket",
        "notifications",
        "realtime",
        "chat",
        "broadcasting",
        "php-ratchet"
    ],
    "authors": [
        {
            "name": "Your Name",
            "email": "your-email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/console": "^9.0|^10.0|^11.0",
        "illuminate/broadcasting": "^9.0|^10.0|^11.0",
        "illuminate/redis": "^9.0|^10.0|^11.0",
        "illuminate/queue": "^9.0|^10.0|^11.0",
        "illuminate/cache": "^9.0|^10.0|^11.0",
        "ratchet/pawl": "^0.4",
        "ratchet/rfc6455": "^1.0",
        "react/socket": "^1.11",
        "firebase/php-jwt": "^6.0",
        "predis/predis": "^2.0",
        "prometheus/client_php": "^2.6",
        "guzzlehttp/guzzle": "^7.0",
        "ext-redis": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.5|^10.0",
        "mockery/mockery": "^1.4",
        "laravel/sanctum": "^3.0",
        "phpstan/phpstan": "^1.0",
        "squizlabs/php_codesniffer": "^3.6",
        "dms/phpunit-arraysubset-asserts": "^0.4"
    },
    "autoload": {
        "psr-4": {
            "LaravelNotify\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelNotify\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "analyse": "vendor/bin/phpstan analyse src --level=5",
        "format": "vendor/bin/phpcbf --standard=PSR12 src",
        "lint": "vendor/bin/phpcs --standard=PSR12 src"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelNotify\\LaravelNotifyServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}